<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.edubank">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.EduBank">
        <activity android:name=".FinalGPA" />
        <activity android:name=".StudentSemesterResults" />
        <activity android:name=".CalculateGPA" />
        <activity android:name=".NameUniversity" />
        <activity android:name=".GradePointsAdd"/>
        <activity android:name=".GPASettings" />
        <activity android:name=".GPAMainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>